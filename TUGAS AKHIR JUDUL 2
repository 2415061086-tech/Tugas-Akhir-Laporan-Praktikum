#include <iostream>
using namespace std;

int main() {
    
    int harga[] = {12000, 5000, 7500, 20000, 15000};
    int n = sizeof(harga) / sizeof(harga[0]);

    cout << "Daftar harga sebelum diurutkan: \n";
    for (int i = 0; i < n; i++) {
        cout << "Rp" << harga[i] << " ";
    }
    cout << endl;

   
    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - i - 1; j++) {
            if (harga[j] > harga[j + 1]) {
            
                int temp = harga[j];
                harga[j] = harga[j + 1];
                harga[j + 1] = temp;
            }
        }
    }

    cout << "\nDaftar harga setelah diurutkan (termurah -> termahal): \n";
    for (int i = 0; i < n; i++) {
        cout << "Rp" << harga[i] << " ";
    }
    cout << endl;

    return 0;
}
